#include<iostream>
#include<cstdio>
#include<cmath>
using namespace std;

const int MAXN = 1000100;
int dp[MAXN];

void init() {
  for(int i=0; i<MAXN; i++)
    dp[i] = 0;
  for(int i=1; ; i++) {
    int diff = i*(i+2)*(i+1)/6;
    if(diff>=MAXN) break;
    
    
    
  }
  
  
}


int main() {
  init();
  
  
}
