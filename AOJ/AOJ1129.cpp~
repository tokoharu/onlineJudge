#include<iostream>
#include<cstdio>
#include<cmath>
#include<vector>
#include<utility>

using namespace std;
typedef pair<int,int> PII;
typedef long long LL;
#define MP make_pair

int solve() {
  int n,m;
  cin>>n>>m;
  vector<PII> input;
  for(int i=0; i<m; i++) {
    int p,q;
    cin>>p>>q;
    p--;
    input.push_back(MP(p,q));
  }
  
  int pos = 0;
  
  
}


int main() {
  while(1) {
    int ans = solve();
    if(ans<0) break;
    cout<<ans<<endl;
  }
  return 0;
  
}

