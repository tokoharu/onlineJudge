#include<iostream>
#include<cstdio>
#include<cmath>
#include<vector>
#include<set>
#include<utility>
using namespaces std;

#define MP make_pair
typedef pair<double,double> PDD;


void solve(int n) {
  vector<pair<PII,double> >  input ;
  
  for(int i=0; i<n; i++) {
    double p,q,r;
    cin>>p>>q>>r;
    input.push_back(MP(MP(p,q),r));
  }

  
}


int main() {
  while(1) {
    int n;
    cin>>n;
    if(n==0) break;
    solve(n);
  }
  
  
  
  return 0;
}
