#include<iostream>
#include<cstdio>
#include<cmath>
#include<string>
#include<vector>
#include<utility>
#include<set>
#include<algorithm>

using namespace std;
typedef long long LL;
typedef pair<int,int> PII;
#define MP make_pair
int  solve() {
  int W,H,N;
  cin>>W>>H>>N;
  vector<PII> coor;
  for(int i=0; i<N;i++) {
    int p,q;
    cin>>p>>q;
    coor.push_back(MP(p,q));
  }
  
  
}


int main() {
  for(int i=1; ; i++){
    int ans = solve();
    if(ans<0) break;
    cout<<"Case "<<i<<": "<<ans<<endl;
    
  }
  
  return 0;
}
